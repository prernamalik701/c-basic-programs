//print all armstrong number 1 to 500
#include<stdio.h>
int main()
{
    int i,num;
    printf("enter the number through keyboard\n");
    scanf("%d",&num);
    for(i=1;i<=500;i++)
    {
     if(num = (num * num * num) + (num * num * num) + (num * num *num))
     printf("num is armstrong\n");
     else 
     printf("num is not armstrong\n");
    }
    return 0;
}
