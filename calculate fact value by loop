//to find a factorial number

#include<stdio.h>
int main()
{
    int f,i,num;
    printf("enter number through keyboard\n");
    scanf("%d",&num);
    for(i=1;i<=num;i++)
    {
    if(num)
    f = (num -i) ;
    printf("%d",f);
    }
    return 0;
}
