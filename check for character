//determine the type of given input
#include<stdio.h>
int main()
{
    char c;
    printf("enter the character through keyboard\n");
    scanf("%c",&c);
    if(c>=65 && c<=90)
    {
    printf("its a upeer case\n");
    }
    else if(c>=97 && c<=122)
    {
        printf("its a lower case\n");
    }
    else if(c>=48 && c<=57)
    {
        printf("its a digits case\n");
    }
    else if(c>=0 && c<=47 || c>=58 && c<=64 || c>=91 && c<=96 || c>=123 && c<=127)
    {
        printf("its a special case\n");
    }
    return 0;
}
