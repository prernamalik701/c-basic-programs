//check lie region of 3 points
#include<stdio.h>
int main ()
{
    int x1,x2,x3,y1,y2,y3,m,n;
    printf("enter all points through keyboard\n");
    scanf("%d%d%d%d%d%d",&x1,&x2,&x3,&y1,&y2,&y3);
    m = ((y2 - y1)/(x2 - x1));
    printf("value of m is:%d\n",m);
     n = ((y3 - y2)/(x3 - x2));
    printf("value of n is:%d\n",n);
    
   if(m == n)
   printf("all the three points lie on straight line\n");
   
  else
  printf("all the three points do not lie on straight line");
    return 0;
}
