//find absolute value
#include<stdio.h>
int main ()
{
    int num;
    printf("enter the value through kryboard\n");
    scanf("%d",&num);
    if(num >= 0)
    printf("absolute valueis:%d\n",num);
    else
    if(num < 0)
    if(num = (num * (-1)))
    printf("absolute value is :%d",num);
    return 0;
}

